
select * from member_tbl_02;
drop table system.member_tb1_02

create table member_tbl_02 (
    custno    number(6) primary key,
    custname  varchar2(20),
    phone     varchar2(13),
    address   varchar2(60),
    joindate  date,
    grade     char(1),
    city      char(2)
);


